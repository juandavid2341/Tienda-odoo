<odoo>
    <data>
      <!-- explicit list view definition -->
  
      <record model="ir.ui.view" id="odoo_prueba.create_almacenes_list">
        <field name="name">odoo_prueba create almacenes list</field>
        <field name="model">create.almacenes</field>
        <field name="arch" type="xml">
          <tree>
            <field name="name"/>
          </tree>
        </field>
      </record>
  
  
      <record model="ir.ui.view" id="odoo_prueba.create_almacenes_form">
        <field name="name">odoo_prueba create almacenes form</field>
        <field name="model">create.almacenes</field>
        <field name="arch" type="xml">
          <form>
            <sheet>
              <group>
                <field name="name"/>
              </group>
            </sheet>
          </form>
        </field>
      </record>
  
  
      <!-- actions opening views on models -->
  
      <record model="ir.actions.act_window" id="odoo_prueba.create_almacenes_action_window">
        <field name="name">Almacen</field>
        <field name="res_model">create.almacenes</field>
        <field name="view_mode">tree,form</field>
      </record>
  
  
    
  
  
  
    </data>
  </odoo>